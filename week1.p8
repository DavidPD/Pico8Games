pico-8 cartridge // http://www.pico-8.com
version 5
__lua__

t = 0

player_type = {
  anim_frame_delay = 3,
  air_spr={2,3},
  stand_spr=1,
  walk_anim = {1,2,1,3},
  walk_speed = 2, walk_accel = 0.3,

  init = function(self)
    self.last_shot = 0
    self.dir_y = 0
  end,

  shoot = function(self)
    if t - self.last_shot > 3 then
      self.last_shot = t
      add(bullets, {
        x = self.x + self.dir * 2, 
        y = self.y + 1, 
        dir_x = self.dir + self.dir_y * self.dir,
        dir_y = self.dir_y,
        lifetime = 24
      })
      sfx(0)
    end
  end,

  draw = function(self)
    -- draw gun
    if self.dir_y == 0 then
      spr(18, self.x, self.y + 2, 1, 1, self.dir == -1)
    elseif self.dir_y == -1 then
      spr(19, self.x, self.y, 1, 1, self.dir == -1)
    end
  end
}

entities = {}
bullets = {}

function entity(type, x, y)
  e = {
    type = type,
    x = x, y = y,
    vx = 0, vy = 0,
    walking = false,
    dir = 1,
    frame = 1,
    is_grounded = false,
  }
  if type.init != nil then type.init(e) end
  return e
end

function _init()
  player = entity(player_type, 40, 30)
  add(entities, player)
end

function _update()
  -- update time
  t += 1;

  -- handle input
  if btn(0) then
    player.dir = -1
    player.walking = true
    if not player.walking then
      player.frame = 0
    end
  elseif btn(1) then
    player.dir = 1
    player.walking = true
    if not player.walking then
      player.frame = 0
    end
  else
    player.walking = false
  end

  if btn(2) then player.dir_y = -1 else player.dir_y = 0 end

  if btn(4) and player.is_grounded then
    player.vy = -3.5
    player.is_grounded = false
  end

  if btn(5) then
    player.type.shoot(player)
  end

  for e in all(entities) do
    if e.type.update != nil then
      e.type.update(e)
    else
      update(e)
    end
  end

  for b in all(bullets) do
    update_bullet(b)
  end
end

function _draw()
  cls()
  camera(clamp(player.x - 64, 0, 32), clamp(player.y - 64, -16, 0))
  map(0,0,0,0,32,16)
  draw_entity(player)
  print(player.dir_y, 0, 0, 7)
  for b in all(bullets) do
    bspr = 21
    if (abs(b.dir_x) < abs(b.dir_y)) bspr = 22
    spr(bspr, b.x, b.y)
  end
end

function update(e)
  printh(#bullets)
  if e.walking then
    e.vx = clamp(e.vx + e.type.walk_accel * e.dir, -e.type.walk_speed, e.type.walk_speed)
  else
    e.vx *= 0.8
    if abs(e.vx) < 1 then e.vx = 0 end
  end
  e.x += e.vx
  if check_map_collision(e) then
    repeat
      e.x -= sign(e.vx)
    until not check_map_collision(e)
    e.vx = 0
  end

  e.y += e.vy
  if check_map_collision(e) then
    repeat
      e.y -= sign(e.vy)
    until not check_map_collision(e)
    e.is_grounded = e.vy > 0
    e.vy = 0
  end

  e.vy = clamp(e.vy + 0.3, -4, 5)

  if t % e.type.anim_frame_delay == 0 and e.walking then
    e.frame = (e.frame + 1) % #e.type.walk_anim 
  end
end

function update_bullet(bullet)
  bullet.x += bullet.dir_x * 5
  bullet.y += bullet.dir_y * 5
  bullet.lifetime -= 1
  if bullet.lifetime <= 0 then del(bullets, bullet) end
end

function draw_entity(e)
  if not e.is_grounded then
    local air_spr
    if e.vy < 0 then 
      air_spr = e.type.air_spr[1]
    else
      air_spr = e.type.air_spr[2] 
    end
    spr(air_spr, e.x, e.y, 1, 1, e.dir < 0)
  elseif e.walking then
    spr(e.type.walk_anim[e.frame + 1], e.x, e.y, 1,1, e.dir < 0)
  else
    spr(e.type.stand_spr, e.x, e.y, 1, 1, e.dir < 0)
  end
  if e.type.draw != nil then
    e.type.draw(e)
  end
end

function check_map_collision(e, f)
  f = f or 0
  local is_solid = false
  is_solid = solid(e.x / 8, e.y / 8, f)
  is_solid = (is_solid or solid(e.x / 8, (e.y + 7) / 8, f))
  is_solid = (is_solid or solid((e.x + 7) / 8, (e.y + 7) / 8, f))
  is_solid = (is_solid or solid((e.x + 7) / 8, e.y / 8, f))
  return is_solid
end

function solid(x, y, f)
  local s = mget(x, y)
  local is_solid = fget(s, f)
  return is_solid
end

-- utility functions
function clamp(n, min, max)
  if n < min then return min end
  if n > max then return max end
  return n
end

function sign(n)
  if     n > 0 then return 1
  elseif n < 0 then return -1
  else   return 0
  end
end

