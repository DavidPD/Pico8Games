pico-8 cartridge // http://www.pico-8.com
version 5
__lua__

snake = {{x = 5, y = 5}}
dirs = {
  [-1] = {x = 0, y = 0},
  [0]  = {x = -1, y = 0},
  [1]  = {x = 0,  y = -1},
  [2]  = {x = 1,  y = 0},
  [3]  = {x = 0,  y = 1}
}
newdiri = 1
diri = 1
delta = 0
mouse = {}
snakelen = 5

map_max = 16

game_over = false

function _init()
  spawn_mouse()
end

function _update()
  if game_over then return end
  delta += 1
  if     btn(0) then newdiri = 0
  elseif btn(1) then newdiri = 2
  elseif btn(2) then newdiri = 1
  elseif btn(3) then newdiri = 3
  end

  if newdiri == (diri + 2) % 4 then
    newdiri = diri
  end

  if delta >= 6 then
    delta = 0
    diri = newdiri
    local dir = dirs[diri]
    add(snake, padd(snake[#snake], dir))
    local head = snake[#snake]
    if (peq(head, mouse)) then
      snakelen += 1
      spawn_mouse()
      sfx(0)
    elseif #snake > snakelen then 
      del(snake, snake[1]) 
    end
    if check_collision(head, false) then
      game_over = true
      sfx(1)
    end
  end
end

function _draw()
  cls()
  if game_over then
    print('game_over. score: ' .. #snake)
    return
  end
  map(0, 0, 0, 0, 16, 16)
	for k,s in pairs(snake) do
    if k == #snake then 
      sprot(3, diri, s.x * 8, s.y*8)
    else
      spr(1, s.x * 8, s.y * 8)
    end
  end
  print(mouse.x, 0, 0, 2)
  print("^", 0, 0, 2)
  print(mouse.y, 0, 0, 2)
  spr(2, mouse.x * 8, mouse.y * 8)
end

function spawn_mouse()
  repeat
    mouse = {x = flr(rnd(map_max)), y = flr(rnd(map_max))}
  until not check_collision(mouse)
end

function check_collision(p, include_head)
  if p.x < 0 or p.y < 0 or p.x >= 16 or p.y >= 16 then
    return true
  end
  if include_head == nil then include_head = true end
  local head = snake[#snake]
  for s in all(snake) do
    if peq(p, s) and s != head then
      return true
    end
  end
  
  if fget(mget(p.x, p.y), 0) then return true end
  return false
end

-- Rotate sprite
function sprot(n, r, ox, oy)
  local row = flr(n / 16) * 8
  local col = (n % 16) * 8

  for x = 0,7 do for y = 0,7 do
    local rx, ry
    if     r == 1 then rx, ry = x + col, y + row
    elseif r == 2 then rx, ry = 7 - y + col, 7 - x + row
    elseif r == 3 then rx, ry = 7 - x + col, 7 - y + row
    elseif r == 0 then rx, ry = y + col, x + row
    end
    local color = sget(rx, ry)
    pset(ox + x, oy + y, color)
  end end
end

function padd(p, p2)
  return {x = p.x + p2.x, y = p.y + p2.y}
end

function peq(p1, p2)
  return p1.x == p2.x and p1.y == p2.y
end

